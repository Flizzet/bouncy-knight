package {	import flash.display.MovieClip;	import flash.events.*;	public class plusFifty extends MovieClip	{		public function plusFifty()		{			this.addEventListener(Event.ENTER_FRAME, fadePlusFifty);		}		private function fadePlusFifty(e:Event)		{			if (currentFrame == 35)			{				this.removeEventListener(Event.ENTER_FRAME, fadePlusFifty);				parent.removeChild(this);			}		}	}}