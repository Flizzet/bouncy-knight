package  {		import flash.display.MovieClip;	import flash.events.*;		public class noSkillMessage extends MovieClip	{		public function noSkillMessage()		{			this.addEventListener(Event.ENTER_FRAME, fadeMessageOut);		}		private function fadeMessageOut(e:Event)		{			if (currentFrame == 35)			{				x +=  900;				parent.removeChild(this);				this.removeEventListener(Event.ENTER_FRAME, fadeMessageOut);			}		}	}}