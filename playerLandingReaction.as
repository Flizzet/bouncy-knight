package  {		import flash.display.MovieClip;	import flash.events.*;		public class playerLandingReaction extends MovieClip {						public function playerLandingReaction()		{					this.addEventListener(Event.ENTER_FRAME, FadeCircleOut);		}				private function FadeCircleOut(e:Event)		{			if (currentFrame == 26)			{				this.removeEventListener(Event.ENTER_FRAME, FadeCircleOut)				parent.removeChild(this);			}								}	}	}