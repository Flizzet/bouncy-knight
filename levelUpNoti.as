package  {		import flash.display.MovieClip;	import flash.events.*;		public class levelUpNoti extends MovieClip {						public function levelUpNoti() {			this.addEventListener(Event.ENTER_FRAME, fadeCoinOut);		}				private function fadeCoinOut(e:Event)		{				if (currentFrame == 70)			{				parent.removeChild(this);				this.removeEventListener(Event.ENTER_FRAME, fadeCoinOut)			}	}	}}