package  {		import flash.display.*;	import flash.events.*;	import flash.geom.ColorTransform;		public class plusOne extends MovieClip {						public function plusOne()		{					this.addEventListener(Event.ENTER_FRAME, FadeCircleOut);		}				private function FadeCircleOut(e:Event)		{			if (currentFrame == 35)			{				this.removeEventListener(Event.ENTER_FRAME, FadeCircleOut)				parent.removeChild(this);			}								}	}	}