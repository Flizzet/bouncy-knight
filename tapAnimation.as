package  {		import flash.display.MovieClip;	import flash.events.*;		public class tapAnimation extends MovieClip {						public function tapAnimation()		{					this.addEventListener(Event.ENTER_FRAME, FadeCircleOut);		}				private function FadeCircleOut(e:Event)		{			if (currentFrame == 12)			{				this.removeEventListener(Event.ENTER_FRAME, FadeCircleOut)				parent.removeChild(this);			}								}			}	}