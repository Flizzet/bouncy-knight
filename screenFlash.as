package {	import flash.display.MovieClip;	import flash.events.*;	public class screenFlash extends MovieClip	{		public function screenFlash()		{			this.addEventListener(Event.ENTER_FRAME, fadeFlashOut);		}		private function fadeFlashOut(e:Event)		{			if (currentFrame == 5)			{				parent.removeChild(this);				this.removeEventListener(Event.ENTER_FRAME, fadeFlashOut);			}		}	}}