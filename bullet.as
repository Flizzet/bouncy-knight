package {	import flash.display.MovieClip;	import flash.events.*;	public class bullet extends MovieClip	{		public function bullet()		{			this.addEventListener(Event.ENTER_FRAME, fadeBulletOut);		}		private function fadeBulletOut(e:Event)		{			if (currentFrame == 50)			{				parent.removeChild(this);				this.removeEventListener(Event.ENTER_FRAME, fadeBulletOut);				y = -2000;			}		}	}}