package {	import flash.display.MovieClip;	import flash.events.*;	public class enemySlash extends MovieClip	{		public function enemySlash()		{			this.addEventListener(Event.ENTER_FRAME, fadeSlashOut);		}		private function fadeSlashOut(e:Event)		{			if (currentFrame == 10)			{				parent.removeChild(this);				this.removeEventListener(Event.ENTER_FRAME, fadeSlashOut);			}		}	}}