package {	import flash.display.MovieClip;	import flash.events.*;	public class expPoint extends MovieClip	{		var ySpeed:Number = 5		public function expPoint()		{			this.addEventListener(Event.ENTER_FRAME, fadeExpOut);		}		private function fadeExpOut(e:Event)		{			rotation += 20						ySpeed++;						y += ySpeed;			y -= 15;						if (currentFrame == 50)			{				parent.removeChild(this);				this.removeEventListener(Event.ENTER_FRAME, fadeExpOut);			}		}	}}