package  {		import flash.display.MovieClip;	import flash.events.*;			public class bossNoti extends MovieClip {						public function bossNoti() {			this.addEventListener(Event.ENTER_FRAME, fadeBossNotiOut);		}		private function fadeBossNotiOut(e:Event)		{			if (currentFrame == 50)			{				parent.removeChild(this);				this.removeEventListener(Event.ENTER_FRAME, fadeBossNotiOut);			}		}	}	}