package  {		import flash.display.MovieClip;	import flash.events.*;		public class plusExp extends MovieClip {						public function plusExp() {			this.addEventListener(Event.ENTER_FRAME, fadeExpNotiOut);		}				private function fadeExpNotiOut(e:Event)		{						if (currentFrame == 35)			{				this.removeEventListener(Event.ENTER_FRAME, fadeExpNotiOut)				parent.removeChild(this);			}		}	}	}